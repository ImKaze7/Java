package gui;

public class Empleado {
	private int codigo;
	private String nombre;
	private int categoria;
	private int nrocel;
	
	private static int cantidad;
	
	private static double sueldosNetos;
	
	public static final double desc;

	static{
		cantidad =0;
		sueldosNetos=0.0;
		 desc=0.15;	 
	}

	public Empleado(int codigo, String nombre, int categoria, int nrocel) {
		super();
		this.codigo = codigo;
		this.nombre = nombre;
		this.categoria = categoria;
		this.nrocel = nrocel;
	}
	public Empleado(int codigo, String nombre) {
		this(codigo,nombre,2,999999999);
	}
	public Empleado() {
		this("983548460", "Jose");
	}
}
